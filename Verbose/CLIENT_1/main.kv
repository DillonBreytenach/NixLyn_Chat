#
#-----------------------------------------------------------------------------------------------------
#   CHAT_MSGS
#-----------------------------------------------------------------------------------------------------
#   #   MSG-ITEMS
#-----------------------------------------------------------------------------------------------------
#
<Chat_Msg>:
    rows: 3
    padding: (0, 5, 30, 0) if root.side == "left" else (30, 5, 0, 0)
    font_size: 15
    size_hint: (1, None)
    height: self.minimum_height
    background_color: (0.7, 0.6, 1, 1)
    color: (0, 0, 0, 1)
    MD_RECT_BUTTON:
        size_hint: 2, None
        text: root.user_m[:2]
        font_size: 15
        on_release: print(root.user_m)



    MD_RECT_BUTTON:
        size_hint: 4, None
        text: root.msg_text
        font_size: 15
        text_size: self.size
        background_color: (0.7, 0.6, 1, 1)
        on_release: print(root.msg_text)


    MD_RECT_BUTTON:
        size_hint: 4, None
        text: root.msg_dt
        font_size: 10
        color: (0.5, 0.5, 0.5, 1)
        on_release: print(root.msg_dt)
