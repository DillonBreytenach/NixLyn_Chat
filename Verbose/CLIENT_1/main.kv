<Chat_Buttons>:
    # Default style for the button
    font_size: 20
    size_hint: (1, None)
    height: 50
    background_color: (0.5, 0.5, 0.5, 1)
    color: (0, 0, 0, 1)
#
#-----------------------------------------------------------------------------------------------------
#   #   CONTACTS-ITEMS-OPTIONS
#-----------------------------------------------------------------------------------------------------
#
<TwoButtons>:
    font_size: 30
    size_hint: (1.5, None)
    height: 60
    background_color: (0.5, 0.5, 0.5, 1)
    color: (0, 0, 0, 1)
    spacing: 0
    padding: (5, 20, 5, 20)

    MD_RECT_BUTTON:
        size_hint: 2, None
        text: root.left_text
        font_size: 20
        on_release: root.go_chat(root.left_text) #, print(f'Button {self.text} pressed')
    MD_RECT_BUTTON:
        text: root.right_text
        on_release: root.go_chat(root.left_text), print(f'Button {self.text} pressed') 
#
#-----------------------------------------------------------------------------------------------------
#   #   CONTACTS_LIST-SCROLLER
#-----------------------------------------------------------------------------------------------------
#
<Scroll_Me>:
    viewclass: "TwoButtons"
    RecycleBoxLayout:
        default_size: None, None
        default_size_hint: 1, None
        size_hint_y: None
        orientation: 'vertical'
        height: self.minimum_height
        pos_hint: {'center_x': 0.5, 'center_y': 0.4}
