<ConnectFirst>:
    name: 'conn_first'
    MDFloatLayout:

        MDLabel:
            id: test_conn
            size_hint: 1, None
            pos_hint: {'x': 0.0,'y': 0.9}
            text: "INSERT_IP_AND_HOST"

        MDTextField:
            id: TEST_IP
            hint_text: "IP: ?.?.?.?"
            size_hint: .8, .1
            pos_hint: {'x': 0.0,'y': 0.75}
            active_line: False
            mode: "rectangle"
            text_color_normal: "black"
            color_mode: 'custom'
            line_color_normal: 0,0,0,1
            line_color_focus: 0,0,0,1
            hint_text_color_focus: 0,0,0,1
            hint_text_color_normal: 0,0,0,.8
            color_active: 0,0,0,1
            normal_color: 0,0,0,1
            fill_color: (20/255, 245/255, 209/255, 0.99)




        MDTextField:
            id: TEST_PORT
            hint_text: "PORT: ???"
            size_hint: .8, .1
            pos_hint: {'x': 0.0,'y': 0.65}
            active_line: False
            mode: "rectangle"
            text_color_normal: "black"
            color_mode: 'custom'
            line_color_normal: 0,0,0,1
            line_color_focus: 0,0,0,1
            hint_text_color_focus: 0,0,0,1
            hint_text_color_normal: 0,0,0,.8
            color_active: 0,0,0,1
            normal_color: 0,0,0,1
            fill_color: (20/255, 245/255, 209/255, 0.99)




        MD_RECT_BUTTON:
            text: "TEST_CONNECT"
            size_hint: 0.7, 0.1
            pos_hint: {'x': 0.0, 'y': 0.45}
            on_release: root.get_conn()

